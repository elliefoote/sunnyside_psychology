---
import Layout from "../layouts/Layout.astro";
import PsychologistCard from "../components/PsychologistCard.astro"
import ServicesCard from "../components/ServicesCard.astro"
import Button from "../components/Button.astro"
---

<Layout title="Sunny Side Psychology">
  <main class="bg-neutral-100 pt-10">
    <div class="m-auto max-w-screen-lg flex flex-col items-center">
<h3 class="font-serif sm:text-5xl text-4xl text-persian-600 text-center">Our Psychologists</h3>
<PsychologistCard />
<h3 class="font-serif text-5xl text-persian-600">Our Services</h3>
<ServicesCard />
</div>
<div class="flex justify-center pb-10">
<Button buttonText="Book Now" href="mailto:sunnysidepsychology@outlook.com" />
</div>
  </main>
</Layout>
